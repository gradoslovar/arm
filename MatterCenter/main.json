{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "servicePlanName": {
            "type": "string"
        },
        "appLocation": {
            "type": "string"
        },
        "servicePLanSkuName": {
            "type": "string",
            "allowedValues": [
                "S1",
                "S2"
            ]
        }
    },
    "variables": {
        "deploymentApiVersion": "2017-05-10"
    },
    "resources": [
        {
            "name": "webFarm",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "[variables('deploymentApiVersion')]",
            "properties": {
                "mode":"Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/gradoslovar/arm/develop/MatterCenter/resources/webFarm.json",
                    "contentVersion": "1.0.0.0"
                }
            },
            "parameters": {
                "servicePLanName": {"value": "[parameters('servicePlanName')]"},
                "servicePlanSkuName": {"value": "[parameters('servicePLanSkuName')]"}                
            }
        }
        
    ],
    "outputs": {}
}